public class Obj {
	public static void main(String[] args) {
		Member m = new Member("±èÃ¶¼ö", 190);
		System.out.println(m);
	}
}

class Member {
	String name;
	int age;
	
	public Member(String name, int age) {
		this.name = name;
		this.age = age;
	}
	
	@Override
	public String toString() {
		return "Member [name=" + name + ", age=" + age + "]";
	}
}
